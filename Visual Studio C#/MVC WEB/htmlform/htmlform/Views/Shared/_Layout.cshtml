<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="icon" href="@Url.Content("/Content/images/favicon.ico") type="image/x-icon" />
    @Styles.Render("~/Content/css/bootstrap/bootstrap.min.css")
    @Styles.Render("~/Content/css/font-awesome/all.css")
    @Styles.Render("~/Content/css/owl-carousel/owl.carousel.min.css")
    @*@Scripts.Render("~/bundles/modernizr/font-awesome/all.css")*@


    @Styles.Render("~/Content/css/style.css")
    @Styles.Render("~/Content/css/media.css")
    @*<link href="Content/css/style.css" rel="stylesheet" type="text/css">
        <link href="Content/css/media.c
            ss" rel="stylesheet" type="text/css">*@

    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">

            </div>
        </div>
        <div class="container body-content">
            @RenderBody()
            <hr />

        </div>


        @Scripts.Render("~/Content/js/jquery.min.js")
        @Scripts.Render("~/Content/js/bootstrap/popper.min.js")
        @Scripts.Render("~/Content/js/bootstrap/bootstrap.min.js")
        @Scripts.Render("~/Content/js/custom.js")
        @Scripts.Render("~/Content/js/owl-carousel/owl.carousel.js")

        @RenderSection("scripts", required: false)
    </body>
</html>
